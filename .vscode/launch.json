{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run EmployeeApp-EmpModule",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/employeeapp-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nsEmpModule/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/EmpModule",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/EmployeeApp/EmpModule\"}"
      },
      "envFile": "${workspaceFolder}/EmpModule/.env1",
      "preLaunchTask": "Build EmployeeApp"
    }
  ]
}